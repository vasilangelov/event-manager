{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validation@1.19.3",
      "destination": "wwwroot/lib/jquery-validation/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "unpkg",
      "library": "font-awesome-5-css@5.4.1",
      "destination": "wwwroot/lib/fontawesome",
      "files": [ "css/all.min.css", "webfonts/*" ]
    },
    {
      "library": "rxjs@7.5.5",
      "destination": "wwwroot/lib/rxjs/",
      "files": [ "dist/bundles/rxjs.umd.min.js" ]
    }
  ]
}
